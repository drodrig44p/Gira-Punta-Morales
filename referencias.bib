isbn = {9786077605645},
issn = {18703453},
journal = {Revista Mexicana de Biodiversidad},
keywords = {abstract,abundancia,communities,difficult to understand and,diversidad verdadera,dominancia,entropy and related measures,equidad,especies equivalentes,especies igualmente comunes,even when they are,of compositional complexity are,often used to assess,riqueza,species diversity in ecological,the shannon index of,their values are not,{\'{i}}ndice de shannon},
number = {1},
